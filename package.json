{
  "name": "universal_react_redux_boilerplate",
  "description": "Boilerplate for a universal React/Redux app",
  "version": "1.0.0",
  "main": "server/index.js",
  "author": "Rob Lee <rob@nodeworks.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run clean && cross-env NODE_ENV=development babel-watch server/index.js",
    "start:prod": "npm run build && npm run serve",
    "serve": "cross-env NODE_ENV=production node dist/index.js",
    "prod": "pm2 delete RRBoilerPlateTmp || true && pm2 start npm --name RRBoilerPlateTmp -- run serve",
    "prod:stage": "pm2 delete RRBoilerPlateTmp || true && pm2 start npm --name RRBoilerPlateTmpTmp -- run serve",
    "prod:delete-tmp": "pm2 delete RRBoilerPlateTmpTmp || true",
    "build": "npm-run-all --parallel build:client build:server --serial build:node",
    "build:client": "rimraf client && cross-env NODE_ENV=production webpack --progress -p --config webpack/client.prod.js",
    "build:server": "rimraf dist && cross-env NODE_ENV=production webpack --progress -p --config webpack/server.prod.js",
    "build:node": "cross-env NODE_ENV=production babel server/index.js -o dist/index.js && babel server/graphql --out-dir dist/graphql && babel server/auth.js -o dist/auth.js",
    "test": "mocha -c test/.setup.js ./src/**/*.spec.js",
    "analyze": "flow check || true",
    "flow:fix": "rm -rf flow-typed/npm && flow-typed update",
    "suite": "npm-run-all --parallel test analyze lint",
    "clean": "rimraf client dist",
    "cm": "git-cz",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "format": "prettier --single-quote --semi=false --write 'src/*.js' && yarn lint",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook",
    "schema:json": "babel-node ./server/graphql/bin/schema_to_json.js",
    "fragments:json": "node ./server/graphql/bin/fragment_type_gen.js",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
