    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.0",
    "react-addons-transition-group": "^15.6.0",
    "react-chartjs-2": "^2.6.1",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.5",
    "react-services-injector": "^1.0.0-rc.3",
    "react-transition-group": "^2.2.0",
    "react-universal-component": "^2.5.0",
    "reactstrap": "^4.8.0",
    "redbox-react": "^1.5.0",
